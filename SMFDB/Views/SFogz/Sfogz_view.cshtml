
@{
    ViewBag.Title = "SFogz";
}


<section id="contact" class="contact">
    <div class="container" data-aos="zoom-out">
        <div class="h-100">
            <div class="py-0 px-5">
                <div id="carouselExampleDark" class="carousel carousel-dark slide mt-4" data-bs-ride="carousel">
                    <div class="carousel-inner">
                        <div class="carousel-item active ">
                            <div class="row align-items-center justify-content-center">
                                <div class="col-12 text-center opacity-95">
                                    <img src="~/img/sf_ogz/Slide2.png" width="85%" height="85%" />
                                </div>
                            </div>
                        </div>
                        <div class="carousel-item">
                            <div class="row align-items-center justify-content-center">
                                <div class="col-12 text-center opacity-95">
                                    <img src="~/img/sf_ogz/Slide3.png" width="85%" height="85%" />
                                </div>
                            </div>
                        </div>
                        <div class="carousel-item">
                            <div class="row align-items-center justify-content-center">
                                <div class="col-12 text-center opacity-95">
                                    <img src="~/img/sf_ogz/Slide4.png" width="85%" height="85%" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleDark" data-bs-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Previous</span>
                    </button>
                    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleDark" data-bs-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Next</span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</section><!-- End Contact Section -->
