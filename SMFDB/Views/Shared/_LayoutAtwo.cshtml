<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @*<title>@ViewData["Title"] - - SDT Smart Factory Portfolio</title>*@
    <!-- JS -->
    <script src="~/lib/jquery/dist/jquery.js"></script>

    <!-- Vendor JS Files -->
    <script src="~/lib/assets/vendor/aos/aos.js"></script>
    <script src="~/lib/assets/vendor/bootstrap/js/bootstrap.js"></script>
    <script src="~/lib/assets/vendor/glightbox/js/glightbox.js"></script>
    <script src="~/lib/assets/vendor/swiper/swiper-bundle.min.js"></script>

    <!-- Template Main JS File -->
    <script src="~/lib/assets/js/main.js"></script>

    <script src="~/lib/vuejs/vue.js"></script>

    <!-- Template Main CSS File -->
    <link href="~/lib/assets/css/style.css" rel="stylesheet">
    <link href="~/lib/assets/css/custom.css?version=1" rel="stylesheet" />
    <link href="~/lib/assets/css/StyleForA9.css" rel="stylesheet" />

    <!-- Vendor CSS Files -->
    <link href="~/lib/assets/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/lib/assets/vendor/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/lib/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Roboto:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Favicons -->
    <link href="~/lib/assets/img/favicon.png" rel="icon">
    <link href="~/lib/assets/img/apple-touch-icon.png" rel="apple-touch-icon">
</head>
<body>
    <header id="header" class="d-flex align-items-center fixed-top">
        <div class="container-fluid d-flex align-items-center justify-content-between">
            <div class="logo">
                <a asp-controller="Atwo" asp-action="Main_atwo_view" class="d-flex align-items-center justify-content-between">
                    <h2 class="bg-primary me-2 py-2 px-3 text-white">S</h2>
                    <div>SMART FACTORY <span>SDT-A2</span></div>
                </a>
            </div>
            <nav id="navbar" class="navbar">
                <ul>
                    <a class="nav-link " asp-area="" asp-controller="SFProject" asp-action="Home_page_menu_view">SDT SMART FACTORY HOMEPAGE</a>
                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav><!-- .navbar -->
        </div>
    </header>

    <main id="main">
        <div id="A9" class="A9">
            <div class="nav-left-sidebar">
                <ul class="mt-4">
                    <li class="nav-item">
                        <a class="nav-link " asp-area="" asp-controller="Atwo" asp-action="Main_atwo_view">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link " asp-area="" asp-controller="Atwo" asp-action="Mrp_atwo_view">MRP Road Map</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link " asp-area="" asp-controller="Atwo" asp-action="Project_action_plan_atwo_view">Project Action Plan</a>
                    </li>

                </ul>
            </div>
            @RenderBody()
            <!-- ======= Hero Section ======= -->

        </div>
    </main>
    @RenderSection("Scripts", required: false)
</body>
</html>
<script>
    if (/MSIE \d|Trident.*rv:/.test(navigator.userAgent)) {
        window.location = 'microsoft-edge:' + window.location;
        setTimeout(function () {
            window.location = 'https://go.microsoft.com/fwlink/?linkid=2135547';
        }, 1);
    }
</script>
